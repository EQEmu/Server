	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) attach",
			"type": "cppdbg",
			"request": "attach",
			"program": "${workspaceFolder}/build/bin/world",
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		},
